{
  "name": "react-parcel-template",
  "version": "1.0.0",
  "description": "A template for a react project",
  "scripts": {
    "parcel": "parcel src/index.html --dist-dir build",
    "watch": "parcel watch src/index.html --dist-dir build",
    "dist": "parcel build src/index.html --dist-dir  dist"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@parcel/transformer-sass": "^2.8.3",
    "eslint": "^8.33.0",
    "eslint-plugin-react": "^7.20.6",
    "miragejs": "^0.1.40",
    "parcel": "^2.8.3",
    "process": "^0.11.10"
  }
}
